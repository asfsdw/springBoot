<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo2.dao.UserDAO">
	<select id="getUserList" resultType="com.example.demo2.dto.UserDTO">
		select * from user3 order by idx desc;
	</select>
	<select id="getUserMid" resultType="com.example.demo2.dto.UserDTO">
		select * from user3 WHERE mid = #{mid} LIMIT 1;
	</select>
	
	
	
	
	<insert id="setUserInput">
		insert into user3 values(default, #{dto.mid}, #{dto.name}, #{dto.age}, #{dto.gender}, #{dto.address});
	</insert>
</mapper>